	<script>
			
		filterProperties["Watch_Type_article_attr"]={
			getURLParam: function(){
				var ataStr=configArray["Watch_Type_article_attr"].join(":");
				return (ataStr != "")?"Watch_Type_article_attr"+"="+ataStr:"";
			},
			processURL: function(str){
				configArray["Watch_Type_article_attr"]=Myntra.Search.getFilterValues(str);
				if (Myntra.Search.Data.optimEnabled) {
					Myntra.Search.Data.initJSON.search.fq.push(("Watch_Type_article_attr" + ':("' + configArray["Watch_Type_article_attr"].join('" OR "') + '")').replace('~-', '-').replace('~/', '\/'));
				}
				return true;
			},
			updateUI: function(){
				//unselect all brands
				$(".article_type_attr .mk-labelx_check").removeClass("checked").addClass("unchecked");

				//update UI selection for brands in configArray
				for(var attr=0;attr<configArray["Watch_Type_article_attr"].length;attr++){
					var selector=configArray["Watch_Type_article_attr"][attr].toLowerCase().replace(/\W+/g, "") + "-ata-filter";
					$("."+selector).removeClass("unchecked").addClass("checked");
				}
				return true;
			},
			setToDefault: function(){
				configArray["Watch_Type_article_attr"]=[];
				return true;
			},
			resetParams: function(){
				configArray["page"]=0;
				return true;
		    },
            updateFilterVisibility: function(filters){
                if((filterClickedName != "" && filterClickedName!="Watch_Type_article_attr") || (filterClickedName != "" && filterClickedName=="Watch_Type_article_attr" && configArray["Watch_Type_article_attr"].length == 0)){
                    $(".mk-Watch_Type_article_attr-filters .mk-labelx_check").each(function (index,element){
                        if(typeof filters.article_type_attributes["Watch_Type_article_attr"] == 'undefined' || typeof filters.article_type_attributes["Watch_Type_article_attr"].values[$(element).attr('data-value')] == 'undefined'){
                            $(element).addClass('disabled');
                        }else{
                            $(element).removeClass("disabled");
                        }
                    });
                }
            },
            setFilterClickedName: function(){
                filterClickedName = "Watch_Type_article_attr";
			},
			updateClearFilterUI: function(){
				var clearFilterEl=$(".mk-Watch_Type_article_attr"+"-filters").closest("li").find(".mk-clear-filter");
				var clearAllFilterEl=$(".mk-clear-all-filters");
				if(configArray["Watch_Type_article_attr"].length >= 1){
					clearFilterEl.removeClass("mk-hide");
				}
				else{
					clearFilterEl.addClass("mk-hide");
				}
				if(Myntra.Search.Utils.getNoOfFiltersSet()){
					clearAllFilterEl.removeClass("mk-hide");
				}
				else{
					clearAllFilterEl.addClass("mk-hide");
				}
			}};
		
			
