	<script>
			configArray["Watch_Type_article_attr"]=[];
		trackingBooleans["Watch_Type_article_attr"]=false;
		articleTypeAttributesList.push("Watch_Type_article_attr");
			configArray["Water_Resistance_article_attr"]=[];
		trackingBooleans["Water_Resistance_article_attr"]=false;
		articleTypeAttributesList.push("Water_Resistance_article_attr");
			configArray["Features_article_attr"]=[];
		trackingBooleans["Features_article_attr"]=false;
		articleTypeAttributesList.push("Features_article_attr");
			configArray["Strap_Type_article_attr"]=[];
		trackingBooleans["Strap_Type_article_attr"]=false;
		articleTypeAttributesList.push("Strap_Type_article_attr");
			configArray["Occasion_article_attr"]=[];
		trackingBooleans["Occasion_article_attr"]=false;
		articleTypeAttributesList.push("Occasion_article_attr");
